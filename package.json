{
  "name": "graphql",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "swc src -d dist",
    "build-dev": "npm run build && NODE_ENV=development node --inspect dist/server.js",
    "dev": "swc src/server.ts --out-dir dist --source-maps",
    "dev:watch": "swc src/server.ts -w -d dist --sync",
    "dev:server": "nodemon src/server.ts",
    "start": "npm run build && NODE_ENV=production node dist/server.js"
  },
  "keywords": [],
  "author": "Marcelo Fabiano",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.56",
    "@types/node": "^20.1.0",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "apollo-server": "^3.12.0",
    "graphql": "^16.6.0",
    "ts-node-dev": "^2.0.0"
  }
}
